@model WebApplication4MCC.Models.Karyawan

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Karyawan</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <h4>KaryawanID</h4>
                <label asp-for="KaryawanID" class="control-label"></label>
                <input asp-for="KaryawanID" class="form-control" />
                <span asp-validation-for="KaryawanID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <h4>Nomor Induk Kependudukan</h4>
                <label asp-for="Nik" class="control-label"></label>
                <input asp-for="Nik" class="form-control" />
                <span asp-validation-for="Nik" class="text-danger"></span>
            </div>
            <div class="form-group">
                <h4>Nama</h4>
                <label asp-for="Nama" class="control-label"></label>
                <input asp-for="Nama" class="form-control" />
                <span asp-validation-for="Nama" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="@Url.Action("")"> Back to List </a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
